{
    "name": "lerna-ts",
    "version": "0.3.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "init": "npm run clean:deps && npx lerna bootstrap --no-ci",
        "tsc:watch": "npx lerna run tsc:clean && npx lerna run --stream tsc:watch",
        "start": "npx lerna run --stream clean && npx lerna run --stream build:dll && npx lerna run --stream run:server",
        "clean:deps": "npx lerna exec 'rm -rf node_modules package-lock.json' && rm -rf node_modules package-lock.json",
        "tsc:clean": "npx lerna run tsc:clean",
        "build": "npx lerna run --stream clean && npx lerna run --stream build && npm run copy",
        "copy": "./scripts/init-build-folder.sh && npx lerna run --stream copy",
        "prod": "node ./dist/server/index.js",
        "release": "CI=true npx lerna version --conventional-commits --include-merged-tags --yes",
        "test": "npx lerna run --stream test"
    },
