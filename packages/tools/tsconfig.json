{
    "extends": "../tsconfig/tsconfig.json",
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": "src",
        "outDir": "build",
        "composite": true,
        "declaration": true,
        "declarationMap": true,
        "strictNullChecks": true,
        "tsBuildInfoFile": "./node_modules/.cache/tsconfig.tsbuildinfo",
        "lib": [
            "ES2015",
            "DOM"
        ]
    },
    "include": [
        "src/**/*"
    ]
}